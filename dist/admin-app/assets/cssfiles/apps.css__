.testimonialSlide .slick-list {padding: 15px 0;}
.testimonialSlide .slick-slide{padding:0 15px;}
.testimonialSlide .slick-prev.slick-arrow {font-size: 18px;	box-sizing: border-box;	position: absolute;	width: 36px; height: 36px; left: -40px;	top: 125px;	line-height: 36px; 
	border: #ccc solid 2px;}

.testimonialSlide .slick-next.slick-arrow {font-size: 18px;	box-sizing: border-box;	position: absolute;	width: 36px; height: 36px; right: -40px; top: 125px; line-height: 36px; 
	border: #ccc solid 2px;}

.testimonialSlide .slick-prev::before{font-family: 'slick'; font-size: 20px; line-height: 1; opacity: .75;  color: #000; position: absolute; top: 6px; left: 5px;}

.testimonialSlide .slick-next::before {font-family: 'slick'; font-size: 20px; line-height: 1; opacity: .75; color: #000; position: absolute; top: 6px; right:5px;}
	
	
	
