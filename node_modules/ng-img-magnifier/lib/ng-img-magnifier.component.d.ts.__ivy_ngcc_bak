import { ElementRef, OnInit } from '@angular/core';
export declare class NgImgMagnifierComponent implements OnInit {
    ngOnInit(): void;
    thumbImage: string;
    fullImage: string;
    top: string;
    right: string;
    lensWidth: string;
    lensHeight: string;
    resultWidth: string;
    resultHeight: string;
    imgWidth: string;
    imgHeight: string;
    constructor();
    resultID: ElementRef;
    imgID: ElementRef;
    zoomDiv: ElementRef;
    showZoom: boolean;
    imageZoom(): void;
    showZoomWin(): void;
    hideZoomWin(): void;
    showLensDiv(): void;
}
