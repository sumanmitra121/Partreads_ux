import { ApplicationRef, ComponentFactoryResolver, ComponentRef, Injector, OnDestroy, RendererFactory2 } from '@angular/core';
import { ToastContainerComponent } from './toast-container/toast-container.component';
export declare class ToastContainerService implements OnDestroy {
    private _document;
    private _factoryResolver;
    private _appRef;
    private _injector;
    private _renderer;
    private _containerElement;
    private _componentRef;
    constructor(rendererFactory: RendererFactory2, _document: any, _factoryResolver: ComponentFactoryResolver, _appRef: ApplicationRef, _injector: Injector);
    readonly ref: ComponentRef<ToastContainerComponent>;
    private readonly containerElement;
    ngOnDestroy(): void;
    private _attach;
    private _detach;
    private _destroyContainer;
}
