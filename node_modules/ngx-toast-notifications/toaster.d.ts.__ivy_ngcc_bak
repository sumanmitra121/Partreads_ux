import { Type } from '@angular/core';
import { ToastConfig } from './toast.config';
import { ToastContainerService } from './toast-container.service';
import { ToastNotificationsConfig } from './toast-notifications.config';
import { Toast } from './toast';
export declare class Toaster {
    private _config;
    private _containerService;
    constructor(_config: ToastNotificationsConfig, _containerService: ToastContainerService);
    open(text: string): Toast | null;
    open(config: ToastConfig): Toast | null;
    open(text: string, config?: ToastConfig): Toast | null;
    open(component: Type<any>, config?: ToastConfig): Toast | null;
}
